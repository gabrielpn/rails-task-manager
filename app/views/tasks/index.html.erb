<%= link_to "Créer une tâche", new_task_path %>
<% @tasks.each do |task| %>
  <h2><%= task.name %></h2>
  <p><%= task.details %></p>
  <%= link_to "Accéder de la tache", task_path(task) %>
  <%= link_to "Updater la tache", edit_task_path(task) %>
  <%= link_to "Détruire la tache", task_path(task), method: :delete %>
<% end %>

